    public String whois(String domainName) {
        
        StringBuilder whoisResult = new StringBuilder("");
        WhoisClient client = new WhoisClient();
        try{
            client.connect(WhoisClient.DEFAULT_HOST);
            String whoisData = client.query("=" + domainName);
            whoisResult.append(whoisData);
            client.disconnect();
        } catch (SocketException e){
            e.printStackTrace();
        } catch (IOException e){
            e.printStackTrace();
        } return whoisResult.toString();
  
    } 
    
    
    
