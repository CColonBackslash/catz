    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        
        progress.setValue(0);
        Double endt = Double.parseDouble(ENDINT.getText());
        String remote = IP.getText();
        int count = 0;
        
        
        int i = 0;
        jep.setText(jep.getText() + " Port scanning started on " + remote + "\n");
        do{
            count++;
            final int percent = count * 100;
            progress.setValue(percent);
            try{
                /*
                *socket will say noy listing on any port not open.
                */
                try (Socket s = new Socket(remote, i)) {
                    jep.setText(jep.getText() +"Port: "+i+ " is open on " + remote + "\n");
                }
                
            } catch(IOException ex){
                jep.setText(jep.getText() + " Port: " + i + " is  not open on " + remote + "\n");
            } 
            i++;
            
        }   while(i <= endt);
    }                                        

